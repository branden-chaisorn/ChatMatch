<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<FindBugsFilter>
    <Match>
        <Class name="~.*\.BuildConfig" />
    </Match>
    <Match>
        <Class name="~.*\.R" />
    </Match>
    <Match>
        <Class name="~.*\.R\$.*" />
    </Match>

    <Match>
        <Class name="~.*Test.*" />
        <Bug pattern="NP_NULL_PARAM_DEREF_NONVIRTUAL" />
    </Match>
    <Match>
        <Class name="~.*Test.*" />
        <Bug pattern="NP_NONNULL_PARAM_VIOLATION" />
    </Match>
</FindBugsFilter>